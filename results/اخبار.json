u0646\u0647\u0627\u06cc\u06cc \u0644\u06cc\u06af \u0642\u0647\u0631\u0645\u0627\u0646\u0627\u0646 \u0627\u0631\u0648\u067e\u0627\u060c \u067e\u0627\u0631\u06cc \u0633\u0646 \u0698\u0631\u0645\u0646 \u062f\u0631 \u0628\u0631\u0646\u0627\u0628\u0626\u0648 \u0645\u0642\u0627\u0628\u0644 \u0631\u0626\u0627\u0644 \u0645\u0627\u062f\u0631\u06cc\u062f \u062a\u062d\u0642\u06cc\u0631 \u0634\u062f \u0648 \u0628\u0627 \u0634\u06a9\u0633\u062a 3 \u0628\u0631 \u06cc\u06a9 \u0627\u0632 \u0631\u0633\u06cc\u062f\u0646 \u0628\u0647 \u0645\u0631\u062d\u0644\u0647 \u0628\u0639\u062f\u06cc \u0627\u06cc\u0646 \u0631\u0642\u0627\u0628\u062a\u200c\u0647\u0627 \u0628\u0627\u0632\u0645\u0627\u0646\u062f. \u067e\u0633 \u0627\u0632 \u0627\u06cc\u0646 \u0634\u06a9\u0633\u062a \u0645\u0627\u0631\u06a9\u0627 \u0627\u0633\u067e\u0627\u0646\u06cc\u0627 \u0648 ESPN \u06af\u0632\u0627\u0631\u0634 \u062f\u0627\u062f\u0646\u062f \u06a9\u0647 \u062f\u0631\u06af\u06cc\u0631\u06cc \u0634\u062f\u06cc\u062f\u06cc \u0628\u06cc\u0646 \u0646\u06cc\u0645\u0627\u0631 \u0648 \u062f\u0648\u0646\u0627\u0631\u0648\u0645\u0627\u00a0 \u062f\u0631 \u0631\u062e\u062a\u06a9\u0646 \u0634\u06a9\u0644 \u06af\u0631\u0641\u062a \u0648 \u062f\u0639\u0648\u0627\u06cc \u0627\u06cc\u0646 2 \u0628\u0627\u0632\u06cc\u06a9\u0646 \u062d\u062a\u06cc \u0641\u06cc\u0632\u06cc\u06a9\u06cc \u0647\u0645 \u0634\u062f. (\u0627\u06cc\u0646\u062c\u0627 \u0628\u062e\u0648\u0627\u0646\u06cc\u062f) \u067e\u0633 \u0627\u0632 \u0627\u0646\u062a\u0634\u0627\u0631 \u0627\u06cc\u0646 \u062e\u0628\u0631\u060c \u0647\u0645 \u062f\u0648\u0646\u0627\u0631\u0648\u0645\u0627 \u0648 \u0647\u0645 \u0646\u06cc\u0645\u0627\u0631 \u0646\u0633\u0628\u062a \u0628\u0647 \u0627\u06cc\u0646 \u062e\u0628\u0631 \u0648\u0627\u06a9\u0646\u0634 \u0646\u0634\u0627\u0646 \u062f\u0627\u062f\u0646\u062f \u0648 \u0622\u0646 \u0631\u0627 \u06a9\u0630\u0628 \u062f\u0627\u0646\u0633\u062a\u0646\u062f.  \u0646\u06cc\u0645\u0627\u0631 \u0628\u0627 \u0627\u0646\u062a\u0634\u0627\u0631 \u0635\u062d\u0628\u062a\u06cc \u062f\u0631 \u0648\u0627\u062a\u0633\u200c\u0627\u067e \u0628\u0627 \u0627\u06cc\u0646 \u062f\u0631\u0648\u0627\u0632\u0647\u200c\u0628\u0627\u0646 \u0627\u06cc\u062a\u0627\u0644\u06cc\u0627\u06cc\u06cc \u0645\u062f\u0639\u06cc \u0634\u062f \u06a9\u0647 \u062e\u0628\u0631 \u0645\u0627\u0631\u06a9\u0627 \u062f\u0631\u0648\u063a \u0628\u0648\u062f\u0647 \u0648 \u0627\u06cc\u0646 \u062e\u0628\u0631 \u0635\u062d\u062a \u0646\u062f\u0627\u0631\u062f. \u062f\u0631 \u0686\u062a \u0648\u0627\u062a\u0633\u200c\u0627\u067e \u0646\u06cc\u0645\u0627\u0631 \u0648 \u062f\u0648\u0646\u0627\u0631\u0648\u0645\u0627 \u0645\u06a9\u0627\u0644\u0645\u0647\u200c\u0627\u06cc \u0635\u0648\u0631\u062a \u06af\u0631\u0641\u062a \u0648 \u062f\u0631\u0648\u0627\u0632\u0647\u200c\u0628\u0627\u0646 \u062a\u06cc\u0645 \u0646\u0648\u0634\u062a: \u0633\u0644\u0627\u0645 \u0646\u06cc\u0645\u0627\u0631. \u0645\u0646 \u0648\u0627\u0642\u0639\u0627 \u0627\u0632 \u0628\u0627\u0632\u06cc \u062f\u06cc\u0631\u0648\u0632 \u0634\u0631\u0645\u0646\u062f\u0647 \u0647\u0633\u062a\u0645. \u0627\u06cc\u0646 \u062e\u0628\u0631 \u0628\u0647 \u0647\u06cc\u0686 \u0648\u062c\u0647 \u0642\u0627\u0628\u0644 \u067e\u0630\u06cc\u0631\u0634 \u0646\u06cc\u0633\u062a. \u0646\u06cc\u0645\u0627\u0631 \u067e\u0627\u0633\u062e \u062f\u0627\u062f: \u062f\u0648\u0633\u062a \u0645\u0646 \u062f\u0631 \u0641\u0648\u062a\u0628\u0627\u0644 \u0647\u0631 \u0627\u062a\u0641\u0627\u0642\u06cc \u0631\u062e \u0645\u06cc\u200c\u062f\u0647\u062f. \u0645\u0627 \u06cc\u06a9 \u062a\u06cc\u0645 \u0647\u0633\u062a\u06cc\u0645 \u0648 \u0647\u0645\u0647 \u0628\u0627 \u062a\u0648 \u062e\u0648\u0627\u0647\u06cc\u0645 \u0628\u0648\u062f. \u062a\u0648 \u0628\u0627\u0632\u06cc\u06a9\u0646 \u0628\u0633\u06cc\u0627\u0631 \u062c\u0648\u0627\u0646\u06cc \u0647\u0633\u062a\u06cc \u0648 \u0628\u0627\u06cc\u062f \u0628\u0631\u062f\u0647\u0627\u06cc \u0632\u06cc\u0627\u062f\u06cc \u0631\u0627 \u062a\u062c\u0631\u0628\u0647 \u06a9\u0646\u06cc. \u0628\u0647 \u0627\u06cc\u0646 \u0627\u0634\u062a\u0628\u0627\u0647\u06cc \u06a9\u0647 \u0645\u0631\u062a\u06a9\u0628 \u0634\u062f\u06cc \u0648\u0627\u06a9\u0646\u0634 \u0646\u0634\u0627\u0646 \u0628\u062f\u0647 \u0648 \u062c\u0644\u0648 \u0628\u0631\u0648. \u062f\u0631 \u0622\u063a\u0648\u0634\u062a \u0645\u06cc\u200c\u06af\u06cc\u0631\u0645. \u0633\u062a\u0627\u0631\u0647 \u0628\u0631\u0632\u06cc\u0644\u06cc \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0636\u0645\u0646 \u0627\u0646\u062a\u0634\u0627\u0631 \u0627\u06cc\u0646 \u0645\u06a9\u0627\u0644\u0645\u0647 \u062f\u0631 \u0627\u06cc\u0646\u0633\u062a\u0627\u06af\u0631\u0627\u0645\u060c \u062f\u0631 \u06cc\u06a9 \u0627\u0633\u062a\u0648\u0631\u06cc \u062f\u06cc\u06af\u0631 \u0646\u0648\u0634\u062a: \u0645\u0646 \u0645\u062a\u0646\u0641\u0631\u0645 \u062f\u0631 \u062e\u0635\u0648\u0635 \u0627\u062e\u0628\u0627\u0631 \u0635\u062d\u0628\u062a \u06a9\u0646\u0645 \u0648\u0644\u06cc \u062e\u0628\u0631 \u0645\u0646\u062a\u0634\u0631 \u0634\u062f\u0647 \u06a9\u0627\u0645\u0644\u0627 \u062f\u0631\u0648\u063a \u0627\u0633\u062a. \u0647\u06cc\u0686 \u062f\u0631\u06af\u06cc\u0631\u06cc \u062f\u0631 \u0631\u062e\u062a\u06a9\u0646 \u0634\u0628 \u06af\u0630\u0634\u062a\u0647 \u0631\u062e \u0646\u062f\u0627\u062f. \u062e\u0628\u0631\u0646\u06af\u0627\u0631\u0627\u0646 \u063a\u06cc\u0631\u062d\u0631\u0641\u0647\u200c\u0627\u06cc \u0628\u0627 \u0627\u0646\u062a\u0634\u0627\u0631 \u0627\u06cc\u0646 \u0627\u062e\u0628\u0627\u0631 \u0633\u0639\u06cc \u062f\u0627\u0631\u0646\u062f \u062e\u0648\u062f\u0634\u0627\u0646 \u0631\u0627 \u0628\u0632\u0631\u06af \u06a9\u0646\u0646\u062f.  \u0627\u0646\u062a\u0647\u0627\u06cc \u067e\u06cc\u0627\u0645/\u00a0\n\n\n", "tags": ["\u062f\u0648\u0646\u0627\u0631\u0648\u0645\u0627", "\u0646\u06cc\u0645\u0627\u0631", "\u067e\u0627\u0631\u06cc \u0633\u0646 \u0698\u0631\u0645\u0646", "\u0631\u0626\u0627\u0644 \u0645\u0627\u062f\u0631\u06cc\u062f"], "date": "3/11/2022 10:45:25 AM", "url"